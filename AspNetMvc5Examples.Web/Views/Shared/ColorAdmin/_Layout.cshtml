    <script src="../assets/js/demo/dashboard-v2.min.js"></script>
    <!-- ================== END PAGE LEVEL JS ================== -->


    <script src="../assets/plugins/highlight/highlight.common.js"></script>
    <script src="../assets/js/demo/render.highlight.js"></script>
    <!-- ================== END PAGE LEVEL JS ================== -->

    @{
        var currentController = this.ViewContext.RouteData.Values["Controller"].ToString();
        var currentAction = this.ViewContext.RouteData.Values["Action"].ToString();
    }


    <script>
        $(document).ready(function () {
            App.init();
            DashboardV2.init();
            Highlight.init();
        });
    </script>
    @RenderSection("scripts", required: false)

